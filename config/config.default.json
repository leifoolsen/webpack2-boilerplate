{
  "server": {
    "host": "localhost",
    "port": 8082,
    "compress": false,
    "contentBase": "src",
    "publicPath": "/",
    "apiPath": "/api",
    "historyApiFallback": {
      "verbose": true,
      "disableDotRule": false
    },
    "proxy": {
      "/api": {
        "target": "http://localhost:8010",
        "changeOrigin": true
      }
    }
  },
  "apiServer": {
    "host": "localhost",
    "port": 8010,
    "apiPath": "/api"
  },
  "logger": {
    "console": {
      "level": "debug"
    }
  }
}
